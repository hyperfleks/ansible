  "api_key": "{{ centrifugo_api_key }}",
  "debug": "false",
  "log_level": "error",
  "admin": false,
  "port": {{ centrifugo_port }},
  "namespaces": [
    {
      "name": "public",
      "anonymous": true,
      "publish": true,
      "presence": true,
