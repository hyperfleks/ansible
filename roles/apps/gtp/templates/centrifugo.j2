    {
      "name": "messages",
      "presence": true,
      "join_leave": true,
      "history_recover": true,
      "history_size": 20,
      "history_lifetime": 30
    },
    {
      "name": "d-edit",
      "presence": true,
      "join_leave": true,
      "history_recover": true,
      "history_size": 20,
      "history_lifetime": 30
    }
  ]
