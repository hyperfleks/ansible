# {{ ansible_managed }}

import os
name = '{{ app_name }}'
bind = '0.0.0.0:{{ gunicorn_port }}'
proc_name = 'django {{ app_name }}'
daemon = True
user = '{{ app_name }}'
group = '{{ app_name }}'

import multiprocessing
workers = multiprocessing.cpu_count() * 2 + 1

errorlog='/var/log/{{ app_name }}/gunicorn_error.log'